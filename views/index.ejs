<%- include('./partial/_head.ejs', {title: 'The Incubator'}) %>
<body>
  <%- include('./partial/_navbar.ejs', {page: 'home'}) %>
    <div class="container mx-auto px-4">
      <div class="relative overflow-x-auto">
        <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
            <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
                <tr>
                    <th scope="col" class="px-6 py-3">
                        Name
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Code
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Location
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Level
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Action
                    </th>
                </tr>
            </thead>
            <tbody>
              <% data.forEach(el => { %>
                <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">
                    <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                        <%= el.name %>
                    </th>
                    <td class="px-6 py-4">
                        <%= el.code %>
                    </td>
                    <td class="px-6 py-4">
                        <%= el.location %>
                    </td>
                    <td class="px-6 py-4">
                        <%= el.level %>
                    </td>
                    <td class="px-6 py-4">
                      <button type="button" class="py-2.5 px-5 me-2 mb-2 text-sm font-medium text-gray-900 focus:outline-none bg-white rounded-lg border border-gray-200 hover:bg-gray-100 hover:text-blue-700 focus:z-10 focus:ring-4 focus:ring-gray-100 dark:focus:ring-gray-700 dark:bg-gray-800 dark:text-gray-400 dark:border-gray-600 dark:hover:text-white dark:hover:bg-gray-700">See Details</button>
                    </td>
                </tr>
              <% }) %>
            </tbody>
        </table>
      </div>
    </div>
</body>
</html>